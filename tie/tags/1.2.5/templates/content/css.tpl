body {background: #DBDADA;color: #4B4B4B;font-family: "Trebuchet MS", Verdana, Tahoma, Arial, Sans-serif;font-size: 12px;margin: 0 0 10px;}.docs fieldset {padding: 1em;}.docs fieldset legend {font-size: 1.2em;}.docs fieldset fieldset {margin-top: 15px;}.docs fieldset fieldset legend {font-size: 0.9em;}.docs fieldset code {margin-bottom: 5px;}.docs fieldset ul {margin-left: 10px;}.feed {background: url("/images/feed-icon-14x14.png") no-repeat 0 50%;margin-left: 3px;padding: 0 0 0 19px;}fieldset {border: 2px solid #FEE498;color: #606060;font-size: 13px;margin-bottom: 5px;padding: 10px;}img {border: none;}input, select, textarea {border: 1px solid #C9C8C8;color: #4B4B4B;font-size: 11px;line-height: 14px;margin: 0;padding: 2px 0 2px 4px;}legend {color: #606060;font-size: 12px;font-weight: 700;margin: 0;padding: 0;}optgroup {font-style: normal;font-weight: 400;}select {padding: 1px 0 0 4px !important;}#content {padding: 8px 8px 0;}#footer {background-color: #E7E5E5;border-bottom: 1px solid #B7B6B6;border-top: 1px solid #B7B6B6;color: #333;line-height: 21px;padding: 10px 0 7px;text-align: center;}#footer p.nav a {color: #4997CD;padding: 0 10px;text-decoration: none;}#header {background: #FFF;color: #000;height: auto !important;overflow: hidden;}#header .left {float: left;padding: 21px 0 0 25px;position: static;}#header .right {background: #FFF;color: #DBDADA;float: right;padding: 10px 10px 0 0;text-align: right;}#nav {background: #4997CD;border-bottom: 4px solid #217CBB;margin: 0;overflow: hidden;position: static;white-space: nowrap;}#nav ul {background-color: #4997CD;margin: 0;padding: 0;}#nav ul li {background-color: #4997CD;float: left;font-size: 15px;font-weight: 700;line-height: 31px;list-style-type: none;}#nav ul li a {background: #4997CD;color: #fff;display: block;margin: 0;padding: 0 22px;text-decoration: none;}#nav ul li a: hover {background: #217CBB;padding: 0 22px;}#nav ul li.selected a {background: #217CBB;color: #FFF;float: left;font-size: 15px;font-weight: 700;line-height: 31px;list-style: none;}#nav ul li: hover {background-color: #217CBB;float: left;font-size: 15px;font-weight: 700;line-height: 31px;list-style-type: none;}#redirect {background-color: #E3EDFC;border: 2px solid #ABE1FA;font-size: 15px;font-weight: 700;line-height: 24px;margin: 8px;padding: 23px;text-align: center;}table.dashboard {border: 1px solid #000;border-collapse: collapse;width: 100%;}table.dashboard th, table.dashboard td.category {font-weight: 700;}table.dashboard td {padding: 1em;}.btn {border: 1px solid #9C9B9B;color: #4B4B4B;height: 22px;padding: 0 0 1px;}.btnSearch {background-color: #9C9B9B;color: #fff;height: 22px;padding: 0 0 1px;width: 50px;}.btnUpdate {background: #F0F0F0;border: 1px solid #9C9B9B;color: #4B4B4B;height: 22px;padding: 2px;}#container {color: #666;margin: 60px auto 20px;min-width: inherit;width: 33%;}.content {float: left;margin: 0;padding: 0 10px 0 13px;width: 70%;}a.current {background-color: #9C9B9B;color: #fff;padding: 2px 8px;text-decoration: none;}#dialog {background: #fff;border: 2px solid #4997CD;color: #000;padding: 15px 20px 10px;text-align: left;}h1.title {background: #FFF;color: #4997CD;font-size: 26px;margin: 0;padding: 0;}h1.title a {background: #FFF;color: #4997CD;text-decoration: none;}.info {margin: 0 0 0 25%;padding: 9px 10px 0 13px;}.intro {font-family: arial;margin: auto;padding-top: 7%;width: 90%;}a.link {background-color: #4997CD;color: #fff;padding: 2px 8px;text-decoration: none;}a.link: hover {background-color: #9C9B9B;}.links {float: left;margin: 0;padding: 1px 10px 0 0;width: 25%;}.list {background-color: #F1F1F1;color: #9C9B9B;border: 1px solid #B7B6B6;line-height: 19px;margin: 0 0 8px;width: 100%;}.list-entry {background-color: #F1F1F1;border-bottom: 1px solid #fff;clear: both;margin: 0 0 1px;padding: 0 0 9px;position: static;}.list-entry a {color: #9C9B9B;text-decoration: none;}.list-entry td {padding: 10px;}.list-entry: hover {background: #EAFCFF;}.languages {padding: 6px 0 0;}.section {background-color: #fff;border: 1px solid #B7B6B6;line-height: 19px;margin: 0 0 8px;padding: 11px 14px 10px;}.section h2 {color: #4997CD;font-size: 18px;font-weight: 400;margin: 0 0 8px;padding: 0;}.section h2 a, .section p a, #footer a, .list-errorlog, .list-header a, .info a, .content a {color: #4997CD;text-decoration: none;}.section h2 a: hover {color: #217CBB;}.section p {line-height: 19px;margin: 0 0 10px;}.section p a: hover {color: #4997CD;text-decoration: underline;}.section p.news {background-image: url(images/news.gif);background-position: top left;background-repeat: no-repeat;padding: 0 0 0 26px;}.section p.news a {font-style: italic;padding: 0 0 0 8px;}.slogan {background: #FFF;color: #868788;font-style: italic;line-height: 15px;padding: 4px 0 0;position: relative;}.space {background-color: #fff;float: left;height: 31px;width: 246px;}.list-header {clear: both;padding: 3px 0 0 13px;}.list-header, .list-footer {background-color: #fff;height: 29px;line-height: 29px;padding: 2px 0 0 7px;}.list-header td, .list-footer td {padding: 2px;}.list-footer {clear: both;padding: 2px 0 0 13px;}input.txtSearch {width: 106px;}.welcome {background-color: #fff;color: #4997CD;height: auto;line-height: 21px;margin: 0;padding: 0 0 4px;text-align: right;}.textarea {background: #EFEFEF;}.textarea: hover, .textarea: focus {border: 1px solid #4997CD;background: #FFF;}.welcome .menu {padding: 0 10px 0 0;}.welcome a {background-color: #fff;color: #4997CD;text-decoration: none;}*, form, p, #footer p {margin: 0;padding: 0;}