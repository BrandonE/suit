RewriteRule ^forum/([^/]+?)/[^/]+?/([^/]+?)/([^/]+?)/$ index.php?cmd=forum&id=$1&start=$2&limit=$3 [L]
RewriteRule ^forum/([^/]+?)/[^/]+?/([^/]+?)/([^/]+?)/([^/]+?)/$ index.php?cmd=forum&id=$1&start=$2&limit=$3&order=$4 [L]
RewriteRule ^forum/([^/]+?)/[^/]+?/([^/]+?)/([^/]+?)/([^/]+?)/([^/]+?)/$ index.php?cmd=forum&id=$1&start=$2&limit=$3&order=$4&check=$5 [L]
