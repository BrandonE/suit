RewriteRule ^topic/([^/]+?)/[^/]+?/([^/]+?)/([^/]+?)/([^/]+?)/$ index.php?cmd=topic&id=$1&start=$2&limit=$3&order=$4 [L]
RewriteRule ^topic/([^/]+?)/[^/]+?/([^/]+?)/([^/]+?)/([^/]+?)/([^/]+?)/$ index.php?cmd=topic&id=$1&start=$2&limit=$3&order=$4&check=$5 [L]
RewriteRule ^topic/([^/]+?)/[^/]+?/([^/]+?)/([^/]+?)/([^/]+?)/([^/]+?)/([^/]+?)/$ index.php?cmd=topic&id=$1&start=$2&limit=$3&order=$4&check=$5&search=$6 [L]
