RewriteRule ^newtopic/([^/]+?)/[^/]+?/$ index.php?cmd=newtopic&id=$1 [L]
RewriteRule ^notes/$ index.php?cmd=notes [L]
RewriteRule ^profile/([^/]+?)/[^/]+?/$ index.php?cmd=profile&id=$1 [L]
RewriteRule ^recache/$ index.php?cmd=recache [L]
RewriteRule ^register/$ index.php?cmd=register [L]
RewriteRule ^subscribe/([^/]+?)/[^/]+?/$ index.php?cmd=subscribe&id=$1 [L]
RewriteRule ^topic/([^/]+?)/[^/]+?/$ index.php?cmd=topic&id=$1 [L]
