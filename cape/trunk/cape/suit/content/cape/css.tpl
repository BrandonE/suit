div.community div.breadcrumb{	margin: 10px;	padding: 5px;}div.community h2{	color: #9C9B9B;}div.community .list a{	text-decoration: underline;	color: #9C9B9B;}div.community div.new-buttons{	margin: 0 0 4.5em;}div.community div.new-buttons div{	float: left;	margin: 5px;}div.community div.post-controls{	padding-bottom: 0;	text-align: right;}div.community div.signature{	border-top: 1px solid;	margin-top: 16px;}div.community-forum{	background: #EFEFEF;	color: #000;}div.community-forum .forum-title{	font-size: 16px;}div.community-forum div.forum-description,div.post-content{	margin-left: 15px;}div.community table.list table.list-header{	background: #f1f1f1;	height: 100%;}.community-newbutton{	background:#4997CD;	font-weight:700;	margin-bottom:5px;	text-align:center;	width:100px;}div.community-newbutton a{	background: #4997CD;	color: #FFF;	display: block;	padding: 5px;	text-decoration: none;}div.community-newbutton a:hover{	background: #217CBB;}.community-newbuttonlocked{	background:#9C9B9B;	font-weight:700;	margin-bottom:5px;	text-align:center;	width:100px;	color: #FFF;	display: block;	padding: 5px;	text-decoration: none;}*.google{    float: left;    margin-left: 20px;    margin-top: 9px;}fieldset a{	background-color: #FFF;	color: #4997CD;	text-decoration: none;}ul{    margin-left: 15px;}.post{	border-top: 1px solid;	padding-top: 20px;	padding-bottom: 3px;	margin-bottom: 15px;}